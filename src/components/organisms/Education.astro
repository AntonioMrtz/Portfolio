---
import ExperienceCard from "@/components/molecules/experience-card/ExperienceCard.astro";
import { EDUCATION_ID_TO_DATA } from "@/data/education";
---

{
  Object.values(EDUCATION_ID_TO_DATA).map((v) => (
    <ExperienceCard
      imgSrc={v.iconUrl}
      imgAlt={`${v.label}`}
      title={v.label}
      subheader={v.dates ? [v.entity, v.dates] : [v.entity]}
      description={v.description}
      badges={[]}
      forceTechColumnDisplay={true}
      fixedHeight={500}
    />
  ))
}
