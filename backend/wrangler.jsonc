{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "backend-portfolio-am",
  "main": "src/index.ts",
  "compatibility_date": "2025-10-11",
  "assets": {
    "binding": "ASSETS",
    "directory": "./public"
  },
  "observability": {
    "enabled": true
  },
  "rules": [
    {
      "type": "Text",
      "globs": ["**/*.md", "**/*.excalidraw", "**/eslint.config.js"],
      "fallthrough": true
    }
  ]
}
